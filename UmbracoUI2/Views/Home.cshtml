@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.TextPage>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@using UmbracoUI2.Models
@{
    
    Layout = "_MasterTemplate.cshtml";
}

@using Our.Umbraco.Ditto
@inherits UmbracoTemplatePage
@{
    var model = Model.Content.As<HomeModel>();
    var partialModel = model.ProductContainer == null ? new List<ProductContainer>() : model.ProductContainer;
}


@Html.Partial("_Product Item", partialModel)